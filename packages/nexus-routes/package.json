    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@swc/jest": "^0.2.20",
    "@types/jest": "27.4.1",
    "@types/klaw": "3.0.3",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
