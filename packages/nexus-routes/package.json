    "react": "18.0.0",
    "react-router": "6.3.0",
    "react-router-dom": "^6.3.0",
    "schema-utils": "4.0.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@swc/cli": "0.1.57",
