{
  "name": "nexus-routes",
  "version": "0.0.1",
  "description": "A utility to create routing data",
  "main": "./dist/index.js",
  "exports": {
    "./react-helpers": "./dist/react-helpers.js"
  },
  "engines": {
    "node": "14.18.1"
  },
  "files": [
    "dist",
    "bin",
    "react-helpers.js"
  ],
  "bin": {
    "nexus-routes": "./bin/nexus-routes.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "npm-run-all -l --parallel build:types:watch build:js:watch",
    "lint": "eslint --ext .js,.jsx,.tsx,.ts .",
    "lint:fix": "pnpm lint -- --fix",
    "build": "pnpm build:types && pnpm build:js",
    "run-test": "pnpm build && node --es-module-specifier-resolution=node ./dist/dev.js",
    "build:types": "tsc",
    "build:types:watch": "pnpm build:types --watch --preserveWatchOutput --pretty",
    "build:js:watch": "pnpm build:js -w",
    "build:js": "swc ./src -d dist"
  },
  "keywords": [],
