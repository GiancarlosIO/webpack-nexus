{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "noEmit": true,
    "baseUrl": ".",
    "outDir": "./dist",
    "rootDir": "./src",
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules",
    "dist",
    "out",
  ],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}
