{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "outDir": "./dist",
    "rootDir": "./src",
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules",
    "dist",
    "out",
  ],
  "include": [
    "next-env.d.ts",
    "./src/shared/types/*.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
